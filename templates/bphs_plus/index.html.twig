{% extends 'template.html.twig' %}

{% block content %}

    <div class="row">
        <div class="col-md-12">
            <div class="box box-default bg-gray">
                <div class="box-header with-border">
                    BPHS+ Indicators Cumulative Reach
                    <span class="loading loader"></span>
{#                    <a href="#" title="Click here for selected months' cumulative result"#}
{#                       class="btn btn-sm btn-primary pull-right btn-cum-res">#}
{#                        Cumulative Result#}
{#                    </a>#}
                </div>
                <div class="box-body">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="box-warning box">
                                        <!-- data-route is required for ajax URL -->
                                        <div class="box-body route-url" data-route="ajax_bphs_dashboard">
                                            <!-- data-source is required for the filter-->
                                            <div id="bphs_table_cum" data-fixedCols="">
{#                                                {{ tableCumulative|raw }}#}
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="box box-solid bg-gray-active">
